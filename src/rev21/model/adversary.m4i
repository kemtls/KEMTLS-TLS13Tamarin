rule Reveal_Ltk:
  [ !Ltk($A, ~ltkA) ] --[ RevLtk($A) ]-> [ Out(~ltkA) ]

rule Reveal_PSK:
    [ SecretPSK($A, res_psk)]--[ RevealPSK($A, res_psk) ]->[Out(res_psk)]

// Reveal the KEM secret key
rule Reveal_KemSk:
    [ EKemSk(~esk, ~tid, $A) ] --[ RevEKemSk(~tid, $A, $ekem, ~esk) ]-> [ Out(~esk), EKemSk(~esk, ~tid, $A) ]